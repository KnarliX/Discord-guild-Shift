{
  "name": "discord-guild-shift-bot",
  "version": "1.0.1",
  "description": "Automated Discord server migration and role sync bot.",
  "main": ".build/index.js",
  "type": "module",
  "scripts": {
    "build": "pnpm install --frozen-lockfile && tsc",
    "start": "node ./.build/index.js",
    "dev": "tsc && node ./.build/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "discord",
    "bot",
    "migration",
    "typescript",
    "open-source"
  ],
  "author": "Rajnish Mehta",
  "license": "MIT",
  "engines": {
    "node": ">=20.20.0"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "@vitest/ui": "^4.0.18",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}